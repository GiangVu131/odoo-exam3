<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="popup_plan_sale_order_view_form" model="ir.ui.view">
            <field name="name">popup_plan_sale_order_view_form</field>
            <field name="model">popup.plan.sale.order</field>
            <field name="arch" type="xml">
                <form string="popup_plan_sale_order_form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="plan_sale_order"/>
                            <field name="sale_order_id"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="confirm_plan_sale_order" string="Saveubmit" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="z"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>